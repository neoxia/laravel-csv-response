{
    "name": "neoxia/laravel-csv-response",
    "license": "MIT",
    "description": "Add a CSV response type to Laravel",
    "require": {
        "php": ">=5.5.9",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
         "psr-4": {
            "Neoxia\\": "src/Neoxia/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neoxia\\Routing\\ResponseFactoryServiceProvider"
            ]
        }
    }
}
