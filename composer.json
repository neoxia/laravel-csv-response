{
    "name": "neoxia/laravel-csv-response",
    "license": "MIT",
    "description": "Add a CSV response type to Laravel",
    "require": {
        "php": ">=5.5.9",
        "illuminate/routing": "^5.1|^6.0|^7.0",
        "illuminate/support": "^5.1|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
         "psr-4": {
            "Neoxia\\": "src/Neoxia/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neoxia\\Routing\\ResponseFactoryServiceProvider"
            ]
        }
    }
}
